!iflang [english]

!begin_node SETTINGS

!begin_verbatim
typedef struct Settings
{
  int16_t version;
  int16_t struct_len;
  int16_t boot_rez;
  int16_t falcon_rez;
  union
  {
    struct
    {
      unsigned pulldown          :1;
      unsigned insert_mode       :1;
      unsigned long_titles       :1;
      unsigned alerts_under_mouse:1;
      unsigned fsel_1col         :1;
      unsigned grow_shrink       :1;
      unsigned tear_aways_topped :1;
      unsigned auto_update_shell :1;
      unsigned alert_mode_change :1;
      unsigned ignore_video_mode :1;
      unsigned reserved          :6;
    } s;
    uint16_t i;
  } flags;
  int16_t gadget_pause;                   /* 50 Hz timer tics */
  KEYCODE menu_start;
  KEYCODE app_switch;
  KEYCODE app_sleep;
  KEYCODE ascii_table;
  KEYCODE redraw_all;
  KEYCODE wind_keys[13];

  OB_PREFER color_3D[4];
  OB_PREFER color_root[4];
  OB_PREFER color_exit[4];
  OB_PREFER color_other[4];

  int8_t sort_type
  int8_t find_file[26]
  int8_t fsel_path[10][35]
  int8_t fsel_ext[10][6];

} SETTINGS;
!end_verbatim

